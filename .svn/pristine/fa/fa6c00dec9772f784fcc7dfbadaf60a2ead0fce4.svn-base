	<script>
		$(function(){
			$("#exccmd").click(function(){
				var cmd =  $("#input-cmd").val();
				cmd = $.base64.btoa(cmd);
				alert(cmd);
				if(document.getElementById("input-21").checked){
					var ip = "172.50.20.21";
				}
				else if(document.getElementById("input-22").checked){
					var ip = "172.50.20.22";
				}
				else if(document.getElementById("input-23").checked){
					var ip = "172.50.20.23";
				}
				else{
					alert("No Device Choosed");
				}
				$.post("http://dev.bk.com:8000/exccmd/",{
						'ip' : ip ,
						'cmd' : cmd
				},function(res){
					if(res.result){
						$("#show-result").html(res.excresult);
					}
				},'json');
			})
		})
	</script>
