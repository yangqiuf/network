                            <tr>
                                <td style="width: 100px;">
									<input tabindex="2" type="checkbox" id="input-21"> 
								</td>
                                <td style="width: 20%;">172.50.20.21</td>
                                <td style="width: 20%;">蓝鲸</td>
                                <td style="width: 20%;">在线</td>
